exec --no-startup-id swaymsg 'workspace 1; exec firefox; workspace 3; layout tabbed; exec $term; emacsclient -c; workspace 2; layout tabbed; exec thunderbird; exec Discord'
workspace 1 output HDMI-A-1
workspace 2 output DP-1
workspace 3 output DVI-D-1
workspace 4 output HDMI-A-2
output HDMI-A-1 pos 0,0
output DP-1 pos 2560,0
output DVI-D-1 pos 3840,0
output HDMI-A-2 pos 5120,0

# Switch sinks
bindsym Shift+F1 exec ~/.local/bin/move-pa-sinks.sh alsa_output.pci-0000_00_1b.0.analog-stereo
bindsym Shift+F2 exec ~/.local/bin/move-pa-sinks.sh  alsa_output.usb-Corsair_Corsair_VOID_PRO_Wireless_Gaming_Headset-00.iec958-stereo